{
    "name": "ChromeQuill",
    "version": "1.4",
    "manifest_version": 2,
    "short_name": "ChromeQuill",

    "background": {
        "scripts": [
            "chromeQuill.js"
        ]
    },

    "browser_action": {
        "default_icon": "32.png",
        "default_title": "Open ChromeQuill in a floating window"
    },

    "description": "Make a LaTeX expression and embed, paste, or grab an image",
    
    "icons": {
        "128": "128.png",
        "64": "64.png",
        "48": "48.png",
        "32": "32.png"
    }
 }
